@page "/users/edit/{UserId}"

<UserForm OnValidSubmit="Edit" User="user"></UserForm>

@code {
    [Parameter] public string UserId { get; set; }

    UserDto user;

    protected override void OnInitialized()
    {
        user = new UserDto { UserName="afumfer", FullName="Armando Fumero", EMail="afumfer@gmail.com", UserId = Guid.Parse(UserId) };

        base.OnInitialized();
    }

    private void Edit()
    {
        Console.WriteLine("Edit user");
    }
}
